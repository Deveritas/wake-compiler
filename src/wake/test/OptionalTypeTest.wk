every OptionalTypeTest is:

	Bool -- ~[test optional int never set]~() {
		var Num? = nothing;
		if Num exists {
			return false;
		}
		return true;
	}

	Bool -- ~[test optional int set]~() {
		var Num? = 5;
		if Num exists {
			return Num == 5;
		}
		return false;
	}

	Bool -- ~[test optional int not set will hit else]~() {
		var Num? = nothing;
		if Num exists {} else {
			return true;
		}
		return false;
	}

	Bool -- ~[test optional int set wont hit else]~() {
		var Num? = 5;
		if Num exists {} else {
			return false;
		}
		return true;
	}
