every OptionalTypeTest is:

	Bool -- testOptionalIntNeverSet() {
		var Int? = nothing;
		if Int exists {
			return false;
		}
		return true;
	}

	Bool -- testOptionalIntSet() {
		var Int? = 5;
		if Int exists {
			return Int == 5;
		}
		return false;
	}

	Bool -- testOptionalIntNotSetWillHitElse() {
		var Int? = nothing;
		if Int exists {} else {
			return true;
		}
		return false;
	}

	Bool -- testOptionalIntSetWontHitElse() {
		var Int? = 5;
		if Int exists {} else {
			return false;
		}
		return true;
	}
