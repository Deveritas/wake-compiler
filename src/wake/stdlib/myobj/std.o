var$ b='';

~Printer~ = function(){
	this.`print(Text)` =
	this.`print(Int)` = function(a) {
		if(typeof$ process != 'undefined'
				&& typeof$ process.stdout != 'undefined'
				&& typeof$ process.stdout.write != 'undefined')
			process.stdout.write(''+a);

		else$ b += '' + a;
	};

	this.`printLine(Text)` =
	this.`printLine(Int)` = function(a) {
		if(typeof$ process != 'undefined'
				&& typeof$ process.stdout != 'undefined'
				&& typeof$ process.stdout.write != 'undefined') return$ this.`print(Text)`(a + '\n');
		console.log(b + a);
		b = '';
	};
};

~System~ = function() {
	this.`exit(Int)` = function(a) {
		if(typeof$ process!='undefined' && typeof$ process.exit!='undefined')
			process.exit(a);
	};
};

~Int~ = function(a) {
	this.`squared()` = function() {
		return$ a * a;
	};

	this.`absoluteValue()` = function() {
		return$ a < 0 ? a * -1 : a;
	};

	this.`orMaxOf(Int)` = function(b) {
		return$ a < b ? b : a;
	};

	this.`orMaxOf(Int[])` = function(b) {
		var$ c = 0, d;
		for(d = a; c < b.length; c++) {
			if(a < b[c]) d = b[c];
		}
		return$ d;
	};

	this.`orMinOf(Int)` = function(b) {
		return$ a > b ? b : a;
	};

	this.`orMinOf(Int[])` = function(b) {
		var$ c = 0, d;
		for(d = a; c < b.length; c++) {
			if(a > b[c]) d = b[c];
		}
		return$ d;
	};

	this.`numberOfDigits()` = function(b) {
		var$ b = 1, c = this.`absoluteValue()`();
		while((c /= 10) > 1) b++;
		return$ b;
	};

};

~Text~ = function(a) {

	this.`toUpper()` = function() {
		return$ a.toUpperCase();
	};

	this.`toLower()` = function() {
		return$ a.toLowerCase();
	};

	this.`switchCase()` = function() {
		return$ a.replace(/([a-z])|([A-Z])/g,
			function(b,c){
					return (c) ? b.toUpperCase() : b.toLowerCase()
			}
		);
	};

	this.`trim()` = function() {
